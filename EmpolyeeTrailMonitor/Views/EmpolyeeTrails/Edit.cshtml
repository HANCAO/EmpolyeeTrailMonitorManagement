@model EmpolyeeTrailMonitor.Models.EmpolyeeTrail

@{
    ViewData["Title"] = "编辑数据";
}

<h1>编辑</h1>

<h4>EmpolyeeTrail</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="GPSX" class="control-label"></label>
                <input asp-for="GPSX" class="form-control" />
                <span asp-validation-for="GPSX" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GPSY" class="control-label"></label>
                <input asp-for="GPSY" class="form-control" />
                <span asp-validation-for="GPSY" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BmapLap" class="control-label"></label>
                <input asp-for="BmapLap" class="form-control" />
                <span asp-validation-for="BmapLap" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BmapLng" class="control-label"></label>
                <input asp-for="BmapLng" class="form-control" />
                <span asp-validation-for="BmapLng" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreateTime" class="control-label"></label>
                <input asp-for="CreateTime" class="form-control" />
                <span asp-validation-for="CreateTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreateUser" class="control-label"></label>
                <input asp-for="CreateUser" class="form-control" />
                <span asp-validation-for="CreateUser" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsCar" /> @Html.DisplayNameFor(model => model.IsCar)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="Distance" class="control-label"></label>
                <input asp-for="Distance" class="form-control" />
                <span asp-validation-for="Distance" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DistanceSecond" class="control-label"></label>
                <input asp-for="DistanceSecond" class="form-control" />
                <span asp-validation-for="DistanceSecond" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">返回列表</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
